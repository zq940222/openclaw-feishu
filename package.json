{
  "name": "@m1heng-clawd/feishu",
  "version": "0.1.3",
  "type": "module",
  "description": "Clawdbot Feishu/Lark channel plugin",
  "license": "MIT",
  "files": [
    "index.ts",
    "src",
    "clawdbot.plugin.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m1heng/clawdbot-feishu.git"
  },
  "keywords": [
    "clawdbot",
    "feishu",
    "lark",
    "飞书",
    "chatbot",
    "ai",
    "claude"
  ],
  "openclaw": {
    "extensions": [
      "./index.ts"
    ],
    "channel": {
      "id": "feishu",
      "label": "Feishu",
      "selectionLabel": "Feishu/Lark (飞书)",
      "docsPath": "/channels/feishu",
      "docsLabel": "feishu",
      "blurb": "飞书/Lark enterprise messaging.",
      "aliases": [
        "lark"
      ],
      "order": 70
    },
    "install": {
      "npmSpec": "@m1heng-clawd/feishu",
      "localPath": ".",
      "defaultChoice": "npm"
    }
  },
  "dependencies": {
    "@larksuiteoapi/node-sdk": "^1.30.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "clawdbot": "2026.1.24-2",
    "tsx": "^4.21.0",
    "typescript": "^5.7.0"
  },
  "peerDependencies": {
    "clawdbot": ">=2026.1.24"
  }
}
